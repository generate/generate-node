{
  "name": "generate-node",
  "description": "Generate a node.js project, with everything you need to begin writing code and easily publish the project to npm.",
  "version": "0.1.7",
  "homepage": "https://github.com/generate/generate-node",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-node",
  "bugs": {
    "url": "https://github.com/generate/generate-node/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "lib",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "base-fs-rename": "^0.1.4",
    "common-questions": "^0.2.4",
    "data-store": "^0.16.0",
    "debug": "^2.2.0",
    "extend-shallow": "^2.0.1",
    "generate-collections": "^0.3.4",
    "generate-defaults": "^0.3.1",
    "generate-git": "^0.1.9",
    "generate-license": "^0.1.7",
    "generate-mocha": "^0.2.0",
    "is-valid-app": "^0.1.2",
    "lazy-cache": "^2.0.1",
    "match-file": "^0.2.0",
    "task-prompts": "^0.1.0",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "assemble": "^0.12.0",
    "base-cwd": "^0.2.1",
    "base-generators": "^0.4.0",
    "base-questions": "^0.6.6",
    "engine-base": "^0.1.2",
    "generate": "^0.7.3",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "^0.1.9",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-unused": "^0.1.2",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "generate",
    "node"
  ],
  "verb": {
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "common-questions",
      "generate",
      "generate-git",
      "generate-license",
      "generate-mocha",
      "verb",
      "verb-readme-generator"
    ],
    "run": true,
    "toc": true,
    "layout": "generator",
    "lint": {
      "reflinks": true
    },
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "generate",
        "generate-git",
        "generate-license",
        "generate-mocha"
      ]
    }
  }
}
